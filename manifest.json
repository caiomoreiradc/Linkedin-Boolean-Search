{
    "manifest_version": 3,
    "name": "Linkedin Boolean Search",
    "version": "1.0",
    "description": "Extension to simplify boolean search for jobs on Linkedin platform",
    "author": "Caio Moreira de Carvalho",
    "icons":{
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "/icons/icon256.png"
      },
    "permissions": [
        "tabs",
        "activeTab"
      ]
}